.container

	.background

	.overlay
		img.logo(
		src="/graphics/logo.jpg"
		ga-on="click"
		ga-event-category="TempBusinessCard"
		ga-event-action="Logo"
		)

		.links

			a.fa.fa-email(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Email"
			title="Contact us by Email"
			href="mailto:suicideteam.il@gmail.com"
			target="_blank"
			)

			a.fa.fa-facebook(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Facebook"
			title="Suicide Team Facebook page"
			href="https://www.facebook.com/Suicide-team-Israel-suspension-crew-256939084407530/"
			target="_blank"
			rel="noopener"
			)

			a.fa.fa-instagram(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Instagram"
			title="Suicide Team Instagram feed"
			href="https://www.instagram.com/suicide_team/"
			target="_blank"
			rel="noopener"
			)

			a.fa.fa-youtube(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Youtube"
			title="Suicide Team Youtube channel"
			href="https://www.youtube.com/channel/UC0tWeCikTnJhGVFtT-TkqqQ"
			target="_blank"
			rel="noopener"
			)

			a.fa.fa-vimeo(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Vimeo"
			title="Suicide Team Vimeo channel"
			href="https://vimeo.com/suicideteam"
			target="_blank"
			rel="noopener"
			)

			a(
			ga-on="click"
			ga-event-category="TempBusinessCard"
			ga-event-action="Suspension.org"
			title="International Suspension Alliance homepage"
			href="http://www.suspension.org"
			target="_blank"
			rel="noopener"
			)
				img(
				title="International Suspension Alliance"
				src="graphics/ISA.png"
				)